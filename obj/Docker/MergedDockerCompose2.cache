networks:
  default:
    external:
      name: microservicenet
services:
  tiendaservicios.api.autor:
    build:
      context: Z:\PROGRAMACION\C#\.NETCORE\TiendaServicios
      dockerfile: TiendaServicios.Api.Autor/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: TiendaServicios.Api.Autor
      target: base
    container_name: TiendaServicios.Api.Autor
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: tiendaserviciosapiautor:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/netcoreapp3.1/TiendaServicios.Api.Autor.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 6104
      target: 80
    tty: true
    volumes:
    - /z/PROGRAMACION/C/.NETCORE/TiendaServicios/TiendaServicios.Api.Autor:/app:rw
    - /c/Users/gerol/vsdbg/vs2017u5:/remote_debugger:rw
    - /c/Users/gerol/AppData/Roaming/ASP.NET/Https:/root/.aspnet/https:ro
    - /c/Users/gerol/AppData/Roaming/Microsoft/UserSecrets:/root/.microsoft/usersecrets:ro
    - /c/Program Files/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /c/Users/gerol/.nuget/packages:/root/.nuget/packages:ro
    - /z/PROGRAMACION/C/.NETCORE/TiendaServicios:/src:rw
version: '3.4'